<div class="card">
  <div class=" card medium card-image waves-effect waves-block waves-light">
    <img class="activator" src="http://www.spiria.com/sites/default/files/blog/devenir_developpeur_0.jpg">
  </div>
  <div class="card-content">
    <span class="card-title activator grey-text text-darken-4"><%= @techtalk.title %></span>
    <p><%= @techtalk.description %></p>

    <% @techtalk.tagsArray.each do |tag| %>
      <div class="chip">
        <%= tag %>
      </div>
    <% end %>
  </div>
</div>

